    <!-- 여기서부터 질문게시판 -->
    <div class="container">
    <h1 style="margin-top:100px">FAQ 게시판</h1>
    
    
    <div style="overflow-x:auto; margin-top:50px">
      <table class="width table table-hover table-striped" style="background-color:white;">
        <thead>
          <th height="50">제목</th>
          <th>작성자</th>
          <th>학과</th>
          <th>등록일</th>
        </thead>
      <% @show_faq.each do |f|%>
        <tr onClick="location.href='/faq_show/<%=f.id%>'" style="cursor: pointer;" >
          <td><%=f.title%></td>
          <td><%= f.user.name %></td>
          <td><%= f.user.major %></td>
          <td><%=f.created_at.strftime("%m/ %d/ %Y")%></td>
        </tr>
      <%end%>
      </table>
      
      <div align="center">
        <%= will_paginate @show_faq, renderer: BootstrapPagination::Rails %>
      </div>
      
      
         <% if user_signed_in? %>
            <form action ="/faq_index" method="get">
                <div class="form-group">
                <button type="submit" class="btn btn-default" style ="margin-top: 15px; margin-left:1044px" >글쓰기</button>
                </div>
            </form>
          <% else %>
    				  <li><%= link_to "로그인 하십시오.", new_user_session_path, data: {confirm: "로그인 하시겠습니까?" }%></li>
						
		      <% end %>
          
    </div>
</div>
    
